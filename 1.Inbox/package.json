    "eslint-plugin-unicorn": "^39.0.0",
    "ethlint": "^1.2.5",
    "prettier": "^2.5.0",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-node-dev": "^1.1.8",
